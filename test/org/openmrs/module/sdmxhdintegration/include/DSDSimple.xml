				<structure:TextFormat textType="String"/>
			</structure:Concept>
			
			<structure:Concept id="VALUE_TYPE"  urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].VALUE_TYPE">
				<structure:Name xml:lang="en">Target</structure:Name>
				<structure:Description xml:lang="en">Type of indicator for target setting.</structure:Description>
				<structure:TextFormat textType="Boolean"/>
			</structure:Concept>
						
			<structure:Concept id="INDICATOR"  urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].INDICATOR">
				<structure:Name xml:lang="en">An Indicator</structure:Name>
