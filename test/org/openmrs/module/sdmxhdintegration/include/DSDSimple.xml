				<structure:TextFormat textType="String"/>				
			</structure:Concept>
			
			<structure:Concept id="ORGANIZATION" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].ORGANIZATION">
				<structure:Name xml:lang="en">Providing Organization</structure:Name>
