					attachmentLevel="Observation"	assignmentStatus="Conditional"  />

				<structure:Attribute conceptRef="LOGICAL_VALUE"  conceptSchemeRef="CS_COMMON"  conceptVersion="1.0"
					conceptSchemeAgency="WHO" codelist="CL_LOGICAL_VALUE"
					attachmentLevel="Observation"	assignmentStatus="Conditional"  />
								
				<!-- SERIES -->
				
				<!-- Mandatory  -->
				<structure:Attribute conceptRef="REPEATS" conceptSchemeRef="WHO" conceptVersion="1.0" conceptSchemeAgency="WHO"
					attachmentLevel="Series" assignmentStatus="Mandatory">
					<!-- 0 = there is no REPEATS limit -->
					<structure:TextFormat  textType="Count"/>
				</structure:Attribute>
				
				<!-- Conditional  -->				
				<structure:Attribute conceptRef="OPERAND" conceptSchemeRef="WHO" conceptVersion="1.0" conceptSchemeAgency="WHO"
