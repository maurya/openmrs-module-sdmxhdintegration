				<structure:TextFormat textType="String"/>
			</structure:Concept>			
			
			<structure:Concept id="TIME_PERIOD" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].TIME_PERIOD">
				<structure:Name xml:lang="en">Time period</structure:Name>
				<structure:Description xml:lang="en">Time period.</structure:Description>
			</structure:Concept>
			
			<structure:Concept id="OBS_VALUE" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].OBS_VALUE">
				<structure:Name xml:lang="en">Indicator Value</structure:Name>
				<structure:Description xml:lang="en">Value of indicator.</structure:Description>
				<structure:TextFormat textType="String"/>
			</structure:Concept>
			
			<structure:Concept id="DATE_COLLECT" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].DATE_COLLECT">
				<structure:Name xml:lang="en">Date of Collection</structure:Name>
				<structure:Description xml:lang="en">Date of data collection.</structure:Description>
				<structure:TextFormat textType="Date" pattern="DD/MM/YYYY"/>
			</structure:Concept>
			
			<structure:Concept id="UNIT" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].UNIT">
				<structure:Name xml:lang="en">Unit of measure</structure:Name>
				<structure:Description xml:lang="en">The unit associated with an indicator value.</structure:Description>
				<structure:TextFormat textType="InclusiveValueRange"/>
			</structure:Concept>
			
			<structure:Concept id="REPEATS" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].REPEATS">
				<structure:Name xml:lang="en">Repeating Attribute</structure:Name>
				<structure:Description xml:lang="en">Every indicator attribute is associated with a field indicating whether it repeats (for compatibility with ISO 11179).</structure:Description>
			</structure:Concept>
			
			<structure:Concept id="OPERAND" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].OPERAND">
