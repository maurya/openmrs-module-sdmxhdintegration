				<structure:Name xml:lang="en">Code</structure:Name>
				<structure:Description xml:lang="en">Alphabetic code used for sorting and identifying indicator that can change over time.</structure:Description>
				<structure:TextFormat textType="String"/>
			</structure:Concept>
			
			<structure:Concept id="SPECIAL_VALUE" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].SPECIAL_VALUE">
				<structure:Name xml:lang="en">Special Value</structure:Name>
				<structure:TextFormat textType="String"/>
			</structure:Concept>
			
			<structure:Concept id="LOGICAL_VALUE" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].LOGICAL_VALUE">
				<structure:Name xml:lang="en">Indicator Value of type Logical</structure:Name>
				<structure:TextFormat textType="String"/>
			</structure:Concept>			
			
		</structure:ConceptScheme>

