				<structure:TextFormat textType="String"/>
			</structure:Concept>
			
			<structure:Concept id="GEOGRAPHIC_PLACE_NAME" agencyID="WHO" version="1.0"  urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].GEOGRAPHIC_PLACE_NAME">
