			<structure:Description xml:lang="en">This Concept Scheme contains harmonized data elements.</structure:Description>
			
			<structure:Concept id="DISEASE" urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].DISEASE">
				<structure:Name xml:lang="en">Disease</structure:Name>
				<structure:Description xml:lang="en">Disease name.</structure:Description>
				<structure:TextFormat textType="InclusiveValueRange"/>
			</structure:Concept>
			
			<structure:Concept id="GENDER"  urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].GENDER">
				<structure:Name xml:lang="en">Gender</structure:Name>
				<structure:Description xml:lang="en">Gender of the population for which data are collected.</structure:Description>
				<structure:TextFormat textType="InclusiveValueRange"/>
			</structure:Concept>
			
			<structure:Concept id="PROG"  urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=WHO:COMMON[0.1].PROG">
				<structure:Name xml:lang="en">Programme</structure:Name>
