					attachmentLevel="Series" assignmentStatus="Conditional" />				
				
				<!-- DATASET -->
				
				<!-- Mandatory  -->				
				<structure:Attribute conceptRef="CODE" conceptSchemeRef="CS_COMMON" conceptVersion="1.0"
					conceptSchemeAgency="WHO" attachmentLevel="DataSet" assignmentStatus="Mandatory" />
				
				<!-- Conditional  -->				
			</structure:Components>
